{
  "name": "@pbuilder/sm",
  "version": "1.3.3",
  "description": "A set schematics to create schematics",
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "format": "prettier --write .",
    "build:watch": "rollup -c --bundleConfigAsCjs --watch",
    "publish": "npm run build && cd ../../dist/sm && npm publish --access=public",
    "publish:verdaccio": "npm run build && cd ../../dist/sm && npm publish --registry http://localhost:4873"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./collection.json",
  "dependencies": {
    "@angular-devkit/core": "17.2.0",
    "@angular-devkit/schematics": "17.2.0",
    "typescript": "~5.3.2",
    "ansi-colors": "4.1.3",
    "inquirer": "8.2.6",
    "node-emoji": "2.1.0",
    "ora": "5.4.1",
    "winston": "3.11.0",
    "winston-console-format": "1.0.8"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-swc": "^0.3.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "glob": "^10.3.10",
    "jasmine": "^5.0.0",
    "rollup": "3.29.4",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-tsconfig-paths": "^1.5.2"
  },
  "engines": {
    "node": ">= 20"
  }
}