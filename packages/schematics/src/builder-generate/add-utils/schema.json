{
  "$schema": "http://json-schema.org/schema",
  "$id": "AddUtils",
  "title": "Add utils to the project",
  "type": "object",
  "properties": {
    "packageManager": {
      "description": "The package manager used to install dependencies.",
      "type": "string",
      "enum": [
        "npm",
        "yarn",
        "pnpm",
        "cnpm",
        "bun"
      ],
      "default": "npm"
    }
  },
  "additionalProperties": false
}